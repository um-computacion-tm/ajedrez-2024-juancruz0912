[run]
source = Juego
omit =
    venv/*
    */test*
    */__init__.py
    */coverage
    Pieza.py
[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def setUp
    def test_
    return raw_input
    print
    if __name__ == '__main__':
    def suite
    __str__
    main()
    @.*\.setter
    @.*\.abstractmethod